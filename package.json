{
  "name": "get-your-guide-watch",
  "version": "0.0.1",
  "description": "Lean back and stare at what random Get Your Guide users are doing right now",
  "main": "build/index.js",
  "scripts": {
    "start": "open index.html",
    "dev": "webpack --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taxigy/get-your-guide-watch.git"
  },
  "author": "Rishat Muhametshin <rishatmuhametshin@gmail.com> (http://rishat.us/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/taxigy/get-your-guide-watch/issues"
  },
  "homepage": "https://github.com/taxigy/get-your-guide-watch#readme",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "lodash": "^4.13.1",
    "moment": "^2.14.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "consistent-return": 2,
      "curly": [
        2,
        "multi-line"
      ],
      "eqeqeq": 2,
      "generator-star-spacing": [
        1,
        {
          "before": false,
          "after": true
        }
      ],
      "indent": [
        2,
        2
      ],
      "key-spacing": [
        2,
        {
          "beforeColon": false,
          "afterColon": true,
          "mode": "strict"
        }
      ],
      "keyword-spacing": [
        2,
        {
          "before": true,
          "after": true,
          "overrides": {}
        }
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        2,
        "always"
      ],
      "space-before-function-paren": [
        2,
        {
          "anonymous": "always",
          "named": "never"
        }
      ],
      "strict": 0,
      "vars-on-top": 2,
      "yield-star-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "no-array-constructor": 2,
      "no-extend-native": 2,
      "no-lone-blocks": 2,
      "no-native-reassign": 2,
      "no-new-object": 2,
      "no-new-wrappers": 2,
      "no-octal": 2,
      "no-octal-escape": 2,
      "no-proto": 2,
      "no-param-reassign": 2,
      "no-return-assign": 2,
      "no-sequences": 2,
      "no-trailing-spaces": [
        2,
        {
          "skipBlankLines": true
        }
      ],
      "no-undef": 2,
      "no-undef-init": 2,
      "no-undefined": 2,
      "no-unused-vars": 2,
      "no-void": 2,
      "no-with": 2,
      "react/jsx-curly-spacing": [
        2,
        "never",
        {
          "allowMultiline": false
        }
      ],
      "react/jsx-equals-spacing": [
        2,
        "never"
      ],
      "react/jsx-handler-names": [
        1,
        {
          "eventHandlerPrefix": "handle",
          "eventHandlerPropPrefix": "on"
        }
      ],
      "react/jsx-indent": [
        2,
        2
      ],
      "react/jsx-indent-props": [
        2,
        2
      ],
      "react/jsx-key": 1,
      "react/jsx-no-duplicate-props": [
        2,
        {
          "ignoreCase": true
        }
      ],
      "react/jsx-no-literals": 0,
      "react/jsx-no-undef": 2,
      "react/jsx-pascal-case": 2,
      "react/jsx-space-before-closing": [
        2,
        "always"
      ],
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-did-mount-set-state": 0,
      "react/no-did-update-set-state": 1,
      "react/no-direct-mutation-state": 2,
      "react/no-is-mounted": 2,
      "react/no-multi-comp": [
        1,
        {
          "ignoreStateless": true
        }
      ],
      "react/self-closing-comp": 1,
      "react/wrap-multilines": 1
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "ecmaFeatures": {
      "arrowFunctions": true,
      "blockBindings": true,
      "classes": true,
      "defaultParams": true,
      "destructuring": true,
      "forOf": true,
      "generators": false,
      "modules": true,
      "spread": true,
      "templateStrings": true,
      "jsx": true
    },
    "plugins": [
      "react"
    ]
  }
}
